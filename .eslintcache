otalQuantity += item.count })\n    setItemsQuantity(totalQuantity)\n  }\n\n  const updateTotal = () => {\n    let totalValue = 0;\n    cart.forEach(item => {totalValue += item.count * parseFloat(item.price)})\n    setTotal(totalValue)\n  }\n\n\n  return (\n    <BrowserRouter>\n      <div className=\"App\">\n      <Nav itemsQuantity={itemsQuantity}/>\n      <Switch>\n        <Route exact path=\"/\" component={Home}/>\n        <Route exact path=\"/about\" component={About}/>\n        <Route exact path=\"/store\" component={Store}/> \n        <Route exact path=\"/store/:id\" component={Item}/> \n        <Route \n          exact path=\"/store/:id\" \n          render = {(props) => (\n             <Item {...props}\n              addToCart={addToCart()}\n              />\n          )} /> \n        {/* <Route exact path=\"/store/:id\">\n            <Item addToCart={() => addToCart()} total = {total} />\n        </Route> */}\n        <Route \n          exact path=\"/cart\" \n          render = {(props) => (\n            <Cart {...props}\n            cart = {cart}\n            itemsQuantity = {itemsQuantity}\n            total = {total}\n            addToCart = {() => addToCart()}\n            />\n          )}/>\n\n        <Route exact path=\"/cart\" component={Cart}/>\n      </Switch>\n      </div>\n    </BrowserRouter>\n  );\n}\n\nexport default App;\n","C:\\Users\\Wojtek\\OneDrive\\Dokumenty\\Programing\\react-project-shopping_cart\\src\\components\\Home.js",[],"C:\\Users\\Wojtek\\OneDrive\\Dokumenty\\Programing\\react-project-shopping_cart\\src\\components\\About.js",[],"C:\\Users\\Wojtek\\OneDrive\\Dokumenty\\Programing\\react-project-shopping_cart\\src\\components\\Item.js",["42"],"import React, { useState } from 'react'\r\nimport '../style/Item.css'\r\nimport { Link } from \"react-router-dom\";\r\nimport minerals from '../minerals.json'\r\n\r\nconst Item = (match, addToCart) => {\r\n  const product = minerals[match.match.params.id - 1]\r\n  const [item, setItem] = use